int led=11; 
int led1=10; 
int led2=9; 
int tombol=2; 
void setup(){ 
pinMode(led,OUTPUT); 
pinMode(tombol,INPUT); 
} 
void loop(){ 
 if(digitalRead(tombol)==HIGH){ 
digitalWrite(led,HIGH);
digitalWrite(led1,HIGH);
digitalWrite(led2,HIGH);
 } 
else{ 
 digitalWrite(led,LOW); 
 digitalWrite(led1,LOW); 
 digitalWrite(led2,LOW); 
 } 
}
