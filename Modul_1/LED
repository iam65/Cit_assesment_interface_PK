int led1=11; int i; 
int led2=10; int j; 
int led3=9; int k; 
void setup(){ 
pinMode(led1,OUTPUT);
pinMode(led2,OUTPUT);
pinMode(led3,OUTPUT);

} 
void loop(){ 
  for (i=0; i<=255; i+=5){ 
analogWrite(led1,i);
analogWrite(led2,i);
analogWrite(led3,i);
delay(100); 
 } 
 for (i=255; i>=0; i-=5){ 
analogWrite(led1,i); 
analogWrite(led2,i); 
analogWrite(led3,i); 
delay(100); 
 }
}
